<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane prefHeight="150.0" prefWidth="300.0"
	xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.111"
	fx:controller="org.unibl.etf.gui.plants.controller.AddRegionController">
	<children>
		<Label layoutX="14.0" layoutY="21.0" prefHeight="17.0"
			prefWidth="61.0" style="-fx-alignment: center_right;" text="Biljka:"
			AnchorPane.leftAnchor="14.0" AnchorPane.topAnchor="20.0" />
		<ComboBox fx:id="cbBasis" layoutX="52.0" layoutY="16.0"
			prefHeight="25.0" prefWidth="172.0" AnchorPane.leftAnchor="85.0"
			AnchorPane.rightAnchor="45.0" AnchorPane.topAnchor="14.0" />
		<Button fx:id="btnAddBasis" layoutX="234.0" layoutY="16.0"
			mnemonicParsing="false" onAction="#addBasis" text="+"
			AnchorPane.rightAnchor="14.0" AnchorPane.topAnchor="14.0" />
		<Label layoutX="16.0" layoutY="61.0" text="Broj biljaka:"
			AnchorPane.leftAnchor="14.0" AnchorPane.topAnchor="61.0" />
		<TextField fx:id="txtPlantsNum" layoutX="137.0" layoutY="57.0"
			prefHeight="25.0" prefWidth="203.0" AnchorPane.leftAnchor="85.0"
			AnchorPane.rightAnchor="14.0" AnchorPane.topAnchor="57.0" />
		<Button fx:id="btnSave" layoutX="229.0" layoutY="161.0"
			mnemonicParsing="false" onAction="#save" text="Sacuvajte"
			AnchorPane.bottomAnchor="14.0" AnchorPane.rightAnchor="14.0" />
	</children>
</AnchorPane>
