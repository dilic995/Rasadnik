<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Group?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.ToolBar?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="parentPane" maxHeight="-Infinity"
	maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
	onMouseClicked="#click" onMousePressed="#press" onMouseReleased="#release"
	prefHeight="650.0" prefWidth="1200.0" stylesheets="@../../../../../../../css/catalogue.css"
	xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="org.unibl.etf.gui.plants.controller.DrawRegionsController">
	<children>
		<AnchorPane layoutX="14.0" layoutY="14.0" prefHeight="200.0"
			prefWidth="200.0" styleClass="toolbarPane" AnchorPane.leftAnchor="0.0"
			AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
			<children>
				<ToolBar id="toolbar" layoutX="28.0" prefHeight="40.0"
					prefWidth="200.0" AnchorPane.bottomAnchor="0.0"
					AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
					AnchorPane.topAnchor="0.0">
					<items>
						<Button id="select-button" fx:id="btnSetSelectTool"
							mnemonicParsing="false" onAction="#setSelectTool" />
						<Button id="left-button" fx:id="btnSetPolygonTool"
							mnemonicParsing="false" onAction="#setPolygonTool" text="P" />
						<Separator orientation="VERTICAL" prefHeight="0.0"
							prefWidth="6.0" />
						<Button id="left-button" fx:id="btnUndo" mnemonicParsing="false"
							onAction="#undo" text="&lt;" />
						<Button id="right-button" fx:id="btnRedo" mnemonicParsing="false"
							onAction="#redo" text="&gt;" />
					</items>
				</ToolBar>
			</children>
		</AnchorPane>
		<ScrollPane layoutY="40.0" AnchorPane.bottomAnchor="60.0"
			AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="400.0"
			AnchorPane.topAnchor="40.0">
			<content>
				<AnchorPane id="border-pane" onMousePressed="#aPressed"
					onMouseReleased="#aReleased" prefHeight="620.0" prefWidth="1000.0">
					<children>
						<Group fx:id="elements" />
					</children>
				</AnchorPane>
			</content>
		</ScrollPane>
		<AnchorPane layoutX="720.0" layoutY="561.0" styleClass="buttonPane"
			AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
			AnchorPane.rightAnchor="0.0">
			<children>
				<Button fx:id="btnSave" mnemonicParsing="false" onAction="#save"
					text="Sacuvajte" AnchorPane.bottomAnchor="14.0"
					AnchorPane.rightAnchor="14.0" />
			</children>
		</AnchorPane>
		<TabPane layoutX="800.0" layoutY="40.0"
			AnchorPane.bottomAnchor="60.0" AnchorPane.rightAnchor="0.0"
			AnchorPane.topAnchor="40.0">
			<tabs>
				<Tab closable="false" text="Opste informacije">
					<content>
						<AnchorPane id="center-pane" maxWidth="400.0" minWidth="400.0"
							prefHeight="200.0" prefWidth="400.0">
							<children>
								<AnchorPane layoutX="74.0" layoutY="52.0" prefHeight="256.0"
									prefWidth="256.0" style="-fx-border-color: #d9d9d9;"
									AnchorPane.rightAnchor="74.0">
									<children>
										<ImageView fx:id="imgPhoto" fitHeight="250.0"
											fitWidth="250.0" layoutX="4.0" pickOnBounds="true"
											preserveRatio="true" AnchorPane.bottomAnchor="3.0"
											AnchorPane.leftAnchor="3.0" AnchorPane.rightAnchor="3.0"
											AnchorPane.topAnchor="3.0" />
									</children>
								</AnchorPane>
								<Label fx:id="lblRegion" layoutX="73.0" layoutY="21.0"
									style="-fx-alignment: CENTER; -fx-font-size: 14; -fx-font-family: arial; -fx-font-weight: bold;"
									AnchorPane.leftAnchor="73.0" AnchorPane.rightAnchor="73.0" />
								<Label fx:id="lblName" layoutX="190.0" layoutY="319.0"
									style="-fx-alignment: center; -fx-font-weight: bold; -fx-font-family: arial;"
									AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0" />
								<Label fx:id="lblPlantsNum" layoutX="68.0" layoutY="351.0"
									prefHeight="17.0" AnchorPane.leftAnchor="68.0" />
							</children>
						</AnchorPane>
					</content>
				</Tab>
				<Tab closable="false" text="Odrzavanje">
					<content>
						<AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0"
							prefWidth="200.0">
							<children>
								<Label layoutX="14.0" layoutY="14.0" text="Aktivni planovi:"
									AnchorPane.leftAnchor="14.0" AnchorPane.topAnchor="14.0" />
								<ListView fx:id="lstActivePlans" layoutX="14.0"
									layoutY="31.0" onMouseClicked="#selectActive" prefHeight="88.0"
									prefWidth="372.0" AnchorPane.leftAnchor="14.0"
									AnchorPane.rightAnchor="14.0" AnchorPane.topAnchor="31.0" />
								<Label layoutX="14.0" layoutY="126.0" text="Zakljuceni planovi:"
									AnchorPane.leftAnchor="14.0" AnchorPane.topAnchor="126.0" />
								<ListView fx:id="lstDonePlans" layoutX="14.0" layoutY="143.0"
									onMouseClicked="#selectDone" prefHeight="88.0" prefWidth="372.0"
									AnchorPane.leftAnchor="14.0" AnchorPane.rightAnchor="14.0"
									AnchorPane.topAnchor="143.0" />
								<TableView fx:id="tblTasks" layoutX="14.0" layoutY="261.0"
									prefHeight="134.0" prefWidth="200.0" AnchorPane.leftAnchor="14.0"
									AnchorPane.rightAnchor="14.0">
									<columns>
										<TableColumn fx:id="colActivity" maxWidth="150.0"
											minWidth="150.0" prefWidth="150.0" text="Aktivnost" />
										<TableColumn fx:id="colDateFrom" prefWidth="75.0"
											text="Datum od" />
										<TableColumn fx:id="colDateTo" prefWidth="75.0"
											text="Datum do" />
										<TableColumn fx:id="colDone" prefWidth="75.0"
											text="Obavljeno" />
									</columns>
									<columnResizePolicy>
										<TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
									</columnResizePolicy>
								</TableView>
							</children>
						</AnchorPane>
					</content>
				</Tab>
			</tabs>
		</TabPane>
	</children>
</AnchorPane>
